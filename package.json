{
  "name": "keystroke",
  "main": "./lib/main",
  "version": "0.3.0",
  "description": "define command from keystroke",
  "keywords": [],
  "repository": "https://github.com/t9md/atom-keystroke",
  "license": "MIT",
  "engines": {
    "atom": "^1.19.0"
  },
  "providedServices": {
    "keystroke": {
      "description": "keystroke",
      "versions": {
        "1.0.0": "provideKeystroke"
      }
    }
  },
  "configSchema": {
    "commands": {
      "type": "array",
      "default": [],
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "keystroke": {
            "type": "string"
          },
          "scope": {
            "type": "string"
          }
        }
      }
    }
  },
  "scripts": {
    "lint": "standard lib/*.js spec/*.js"
  },
  "standard": {
    "globals": [
      "atom",
      "describe",
      "jasmine",
      "expect"
    ],
    "ignore": [
      "spec/fixtures/*"
    ]
  },
  "devDependencies": {
    "prettier": "^1.11.1",
    "standard": "^11.0.1"
  }
}
